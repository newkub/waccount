{
  "name": "account.wrikka.com",
  "version": "0.0.1",
  "description": "Account management for wrikka.com",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.5",
  "author": "Wrikka",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/newkub/account.wrikka.com.git"
  },
  "bugs": {
    "url": "https://github.com/newkub/account.wrikka.com/issues"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "format": "biome format --write .",
    "generate": "nuxt generate",
    "lint": "biome lint --write . && vue-tsc --noEmit",
    "postinstall": "bunx taze -r -w -i && nuxt prepare",
    "prepare": "lefthook install",
    "preview": "nuxt preview",
    "review": "bun run format && bun run lint && bun run test && bun run build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@iconify-json/mdi": "^1.2.3",
    "@nuxtjs/color-mode": "^4.0.0",
    "@vueuse/nuxt": "^14.1.0",
    "@workos-inc/node": "^7.77.0",
    "nuxt": "^4.2.2",
    "vue": "^3.5.26",
    "wrangler": "^4.56.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@nuxt/test-utils": "^3.21.0",
    "@unocss/nuxt": "^0.60.4",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.0.11",
    "lefthook": "^2.0.12",
    "taze": "^19.9.2",
    "unocss": "^0.60.4",
    "vite-plugin-checker": "^0.12.0",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.1.8"
  }
}
